{
  "apps": [{
    "name": "dua-dev",
    "script": "npm",
    "args": "run dev",
    "cwd": "/workspaces/v0-remix-of-untitled-chat",
    "instances": 1,
    "autorestart": true,
    "watch": false,
    "max_memory_restart": "1G",
    "env": {
      "NODE_ENV": "development",
      "PORT": "3000"
    },
    "error_file": "./logs/pm2-error.log",
    "out_file": "./logs/pm2-out.log",
    "log_date_format": "YYYY-MM-DD HH:mm:ss",
    "merge_logs": true,
    "restart_delay": 2000,
    "max_restarts": 999999,
    "min_uptime": "10s",
    "listen_timeout": 10000,
    "kill_timeout": 5000
  }]
}
